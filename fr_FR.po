#: gitchecks\app.py:46
msgid ""
"in {file}, line {line}: trailing whitespace within {context_lines} lines of "
"change"
msgstr ""
"dans {file}, ligne {line} : espacement en trop à moins de {context_lines} "
"lignes d'un changement"

#: gitchecks\app.py:56
msgid "in {file}, line {line}: added line uses CRLF line ending"
msgstr "dans {file}, ligne {line} : la ligne ajoutée utilise la fin CRLF"

#: gitchecks\app.py:62
msgid "in {file}, line {line}: added line uses LF line ending"
msgstr "dans {file}, ligne {line} : la ligne ajoutée utilise la fin LF"

#: gitchecks\app.py:67
msgid "in {file}, line {line}: change adds trailing whitespace"
msgstr ""
"dans {file}, ligne {line} : le changement ajoute de l'espacement en trop"

#: gitchecks\app.py:77
msgid "gitchecks called without arguments\n"
msgstr "gitchecks appelé sans argument\n"

#: gitchecks\app.py:95
#, python-format
msgid "%d errors, aborting commit\n"
msgstr "%d erreurs, annule le commit\n"

#: gitchecks\main.py:27
msgid "Couldn't setup the Python path\n"
msgstr "Impossible de définir le path Python\n"
